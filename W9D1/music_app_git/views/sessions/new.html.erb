<h2>Sign In</h2>

<form class="sign_up" action="<%= new_user_url %>" method="GET">
    <input type="submit" value="Go To Sign Up!">
</form>

<form class="sign_in" action="<%= session_url %>" method="POST">
  <input
    type="hidden"
    name="authenticity_token"
    value="<%= form_authenticity_token %>">

  <ul class="username">
    <li><label for="user_email">Email:</label></li>
    <li><input id="user_email" type="text" name="user[email]"></li>
  </ul>

  <ul class="password">
    <li><label for="user_password">Password:</label></li>
    <li><input id="user_password" type="text" name="user[password]"></li>
  </ul>

  <input type="submit" value="Sign In!"/>
</form>