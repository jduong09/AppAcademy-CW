<ul class="feed-header">
  <li><b>Home</b></li>
  <li><h2 class="feed-username">Welcome <%= current_user.username %>!</h2></li>
</ul>

<%= render 'tweets/form' %>

<div class="infinite-tweets">
  <ul id="feed">
    <% @feed_tweets.each do |tweet| %>
      <li><%= render 'tweets/tweet', tweet: tweet %></li>
    <% end %>
  </ul>
  <button class="more-tweets">Load More Tweets...</button>
</div>
